{% extends '::base.html.twig' %}

{% block body -%}
    <h1>D_revenu_usage list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Rdate</th>
                <th>Rminutecommobile</th>
                <th>Rmensuelclientmobile</th>
                <th>Rminutecomfixe</th>
                <th>Rmensuelcomfixe</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('d_revenu_usage_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{% if entity.rDate %}{{ entity.rDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.rMinuteComMobile }}</td>
                <td>{{ entity.rMensuelClientMobile }}</td>
                <td>{{ entity.rMinuteComFixe }}</td>
                <td>{{ entity.rMensuelComFixe }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('d_revenu_usage_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('d_revenu_usage_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('d_revenu_usage_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
